prefix=@prefix@
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: libXML
Version: @version@
Description: libXML library version2.
Requires:
Libs: -L${libdir} -llibxml2
Cflags: -I${includedir}/libxml2
